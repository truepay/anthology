    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Anthology (Memory Registry)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "--backend=memory",
            ]
        },
        {
            "name": "Launch Anthology (Filesystem Registry)",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "args": [
                "--backend=filesystem",
                "--filesystem.basepath=${workspaceFolder}/bucket"
            ]
        },
    ]
}